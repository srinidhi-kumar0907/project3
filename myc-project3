#CODE
def most_frequent(string):
    d = dict()
    for key in string:
        if key not in d:
            d[key] = 1
        else:
            d[key] += 1
    return d

string = input("string=")
res = most_frequent(string)
for key in sorted(res, key=res.get, reverse=True):
    print(key, res[key])
    
#OUTPUT
string=Mississippi 
i=4
s=4
p=2
m=1
